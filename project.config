[project]
	description = use for git testing
[access]
	inheritFrom = All-Projects
[access "refs/*"]
	owner = group user/the-experimentalist-coder
[access "refs/for/refs/*"]
	read = group user/the-experimentalist-coder
	push = group user/the-experimentalist-coder
	label-Code-Review = -2..+2 group user/the-experimentalist-coder
	label-Verified = -1..+1 group user/the-experimentalist-coder
	rebase = group user/the-experimentalist-coder
	removeReviewer = group user/the-experimentalist-coder
	submit = group user/the-experimentalist-coder
[access "refs/heads/*"]
	read = group user/the-experimentalist-coder
	create = group user/the-experimentalist-coder
	push = +force group user/the-experimentalist-coder
	pushMerge = group user/the-experimentalist-coder
[access "refs/tags/*"]
	push = +force group user/the-experimentalist-coder
	pushTag = group user/the-experimentalist-coder
	pushSignedTag = group user/the-experimentalist-coder
[submit]
	action = merge always
